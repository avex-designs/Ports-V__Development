/*========================================

  COLLECTIONS TEMPLATE

========================================*/

#CollectionSection {
  margin-top: $gutters;
}

.collection-hero {
    overflow: hidden;
    margin-right: -$gutters;
    margin-left: -$gutters ;
    width: calc(100% + #{$gutters*2} );
    position: relative;

    .collection-hero__image-wrapper:after {
      padding-bottom: 25%;
    }

    .collection-section-header {
        position: absolute;
        top: 0;
        display: flex;
        height: 100%;
        width: 100%;
        justify-content: center;
        align-items: center;
        color: #fff;
        flex-direction: column;
        padding-top: 4.0625rem;


        .grid-sub-title {
            margin-bottom: .75rem;
        }
    }
}


.pagination {
  display: flex;
  margin-bottom: 3.5rem;
  padding-top: 0;

  span {
    width: 2rem;
    height: 2rem;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 0 !important;
  }

  .page.current {
    color: #fff;
    background-color: #000;
  }

  span.next,
  span.prev {
    display: inline-flex;
    width: auto;

    a {
      display: flex;
      align-items: center;
      justify-content: center;
    }
  }
  span.prev {  margin-right: auto; }
  span.next {  margin-left: auto; }

  .next-arrow,
  .prev-arrow {
    display: block;
    content: ' ';
    width: 4px;
    height: 6px;
    background-repeat: no-repeat;
    background-position: center;
    background-size: contain;
  }
  .next-arrow {
      background-image: url( {{  'pagination-next.svg' | file_url }} ) ;
      margin-left: .5rem;
  }

  .prev-arrow {
      background-image: url( {{  'pagination-prev.svg' | file_url }} ) ;
      margin-right: .5rem;
  }

}



#Collections_grid-container {
    display: flex;

    .filter_menu-container {
        flex: 1 1 25%;

        nav.breadcrumb {
            text-transform: uppercase;
            margin-bottom: 1.5rem;
            opacity: .4;
            font-size: .75rem;
            line-height: 1.125rem;
            letter-spacing: .0625rem;
        }
    }

    .grid-uniform_collections {
        flex: 1 1 75%;
        display: flex;
        flex-wrap: wrap;
        margin-right: -$gutters/2;
        margin-left: -$gutters/2;
        padding-left: 4.25rem;

        .grid-product {
            flex: 0 0 33.33333%;
            padding: 0 $gutters/2;

            .grid-product__meta {
              margin-bottom: 2rem;
            }
        }
    }

    .filter-menu {
        /*min-width: 300px;*/
        border-radius: 0;

        a.filter-clear.ports-btn.clear {
            display: inline-flex;
            margin-top: 2rem;
            background: transparent;
            color: #2d2d2d;
            border: 1px solid #2d2d2d;
            height: 2.5rem;
            width: 8.625rem;
            opacity: .5;
        }

        a#pt-nav-toggle {
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100%;

            .le_toggle {
                padding-left: .375rem;

                img {
                  transition: all 200ms ease-in-out;
                  transform: rotate(-45deg);
                  -webkit-transform: rotate(-45deg);
                }
            }

            &.active {
              border-bottom: 0;

              .le_toggle {
                    img {
                      transform: rotate(0deg);
                      -webkit-transform: rotate(0deg);
                    }
                }
            }
        }

        .pt-mobile-header a.pt-nav-toggle span {
            height: 0;
        }

        .pt-mobile-header a.pt-nav-toggle span:after,
        .pt-mobile-header a.pt-nav-toggle span:before {
            top: 0 !important;
            height: 0.125rem;
            width: 1rem;
        }

        .pt-mobile-header a.pt-nav-toggle span:before {
            transform: rotate(90deg);
            -webkit-transform: rotate(90deg);
        }

        .pt-mobile-header a.pt-nav-toggle.active span:before {
            transform: rotate(45deg);
            -webkit-transform: rotate(45deg);
        }

        .pt-mobile-header a.pt-nav-toggle span:after {
            transform: rotate(0deg);
            -webkit-transform: rotate(0deg);
        }

        .pt-mobile-header a.pt-nav-toggle.active span:after {
            transform: rotate(-45deg);
            -webkit-transform: rotate(-45deg);
        }

        //.has_group_selected .scroll-content,
        .scroll-content.banana { height: auto; }

        //.has_group_selected .scroll-content.banana,
        .scroll-content { height: 0 ; }

        .filter-group {

            ul {  margin-bottom: 1rem; }
            
            .collection-count,
            .filter-clear,
            i.check-icon {  display: none; }

            .selected i.check-icon {  display: initial; }

            &:last-of-type .scroll-content {  border-bottom: 0.0625rem solid rgba(0, 0, 0, 0.25); }

            h6 {
                height: 3.5rem;
                display: flex;
                align-items: center;
                border-top: 0.0625rem solid rgba(0, 0, 0, 0.25);
                position: relative;
                cursor: pointer;

                &:after {
                    display: block;
                    content: ' ';
                    position: absolute;
                    width: .625rem;
                    height: .625rem;
                    background-image: url('{{ "filter-open.svg" | file_url }}');
                    background-repeat: no-repeat;
                    background-position: center;
                    background-size: contain;
                    right: 0;
                }
            }

            h6.select:after {  background-image: url('{{ "filter-close.svg" | file_url }}');  }
        }
    } /* filter menu */

}


@media screen and (max-width: $ports-max-tab ) {

  #CollectionSection {
      margin-top: 1.5rem;
  }

  .collection-hero {
