    background-color: $colorNav;
    opacity: 1;
    @include transition(none);

    /*
    &:before {
      content: "";
      position: absolute;
      left: $gutter / 2;
      right: $gutter;
      bottom: 0;
      display: block;
      background-color: $colorNavText;
      height: 1px;
