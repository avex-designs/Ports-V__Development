	background: #f1f1f1;;
	position: relative;
	font-size: 12px;
	font-weight: 500;
	letter-spacing: 1px;
	text-transform: uppercase;
	transition: box-shadow .3s ease;
	text-align: center;

	@media screen and (max-width: $ports-max-tab ) {
		background: none;
		max-width: none;
		margin: 0;
		padding: 24px 0;
		border-bottom: 1px solid rgba(151,151,151,.25);
		text-align: left;
		line-height: 18px;

		&:after {
			content: '';
			display: block;
			position: absolute;
			top: 50%;
			right: 5px;
			border: solid black;
			border-width: 0 1px 1px 0;
			display: inline-block;
			padding: 4px;
			transform: rotate(-45deg) translateY(-50%);
			-webkit-transform: rotate(-45deg) translateY(-50%);
		}
	}

	@media screen and (min-width: $ports-min-desk ) {
		&:hover {
			background: #fff;
			box-shadow: 0 2px 15px rgba(0, 0, 0, 0.12);
		}
	}

}

.img-icon {
	display: block;
	margin: 0 auto 20px;
	background-repeat: no-repeat;

	@media screen and (max-width: $ports-max-tab ) {
		display: none;
	}

	&.account-addresses {
		width: 40px;
		height: 58px;
		background: url({{ 'adresses.svg' | asset_url }});
	}

	&.account-view-orders {
		width: 47px;
		height: 55px;
		background: url({{ 'history.svg' | asset_url }});
	}

	&.account-view-details {
		width: 46px;
		height: 58px;
		background: url({{ 'details.svg' | asset_url }});
	}
}

.account-overview {
	max-width: 528px;

	@media screen and (max-width: $ports-max-tab ) {
		max-width: none;
	}

	.logout-link {
		display: none;

		@media screen and (max-width: $ports-max-tab ) {
			display: inline-block;
		}
	}

}

.user-info--holder {
	padding-top: 8px;

	p {
		margin-bottom: 1rem;
	}
}

.user-info__name {
	font-size: 12px;
	font-weight: 500;
	letter-spacing: 1px;
	line-height: 18px;
	text-transform: uppercase;
}

.address-lightbox {
	transition: opacity .3s ease;
	position: fixed;
	top: 50%;
	left: -200%;
	transform: translate(-50%, -50%);
	opacity: 0;
	visibility: hidden;
	background: #fff;
	padding: 64px 148px 45px;
	width: 880px;
	z-index: 9999;
	box-shadow: 0 0 10px 1px rgba(0,0,0,.2);

	@media screen and (max-width: $ports-min-desk) {
		position: absolute;
		width: calc( 100vw - 30px );
	}
	@media screen and (max-width: $ports-max-mob) {
		top: 47px;
		padding: 73px ($gutter/2) 45px;
		transform: translateX(-50%);
	}

	&.opened {
		left: 50%;
		visibility: visible;
		opacity: 1;
	}
}

.address-lightbox-opened {
	&:after {
		content: "";
		position: fixed;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		background: rgba(0,0,0,.84);
		z-index: 9998;
	}
}

.address-lightbox__title {
	margin-bottom: 39px;
}

.address-lightbox__cancel {
	position: absolute;
	right: 18px;
	top: 19px;
	width: 28px;
	height: 28px;

	&:hover {
		opacity: .9;
	}

	&:before,
	&:after {
		position: absolute;
		left: 15px;
		content: ' ';
		height: 32px;
		width: 2px;
		background-color: #1d1d1b;
	}

	&:before {
		transform: rotate(45deg);
	}

	&:after {
		transform: rotate(-45deg);
	}

	span {
		display: none;
	}

}

.address-lightbox__button {
	margin-bottom: 16px;
	min-width: 291px;
}

.table--orders {
	background: none;
	font-family: "stevie-sans",sans-serif;
	font-size: 14px;
	font-weight: 300;
	line-height: 17px;
	margin-top: -4px;

	th {
		font-family: "stevie-sans",sans-serif;
		text-transform: none;
		color: rgba(0,0,0,.5);
		letter-spacing: normal;
		font-size: 14px;
		font-weight: 300;
		line-height: 17px;
		padding-left: 0;
		padding-right: 0;

		@media screen and (max-width: $ports-max-mob ) {
			padding-bottom: 8px;
		}
	}

	td {
		padding: 25.5px 0;

		&:first-child {
			font-size: 12px;
			font-weight: 500;
			letter-spacing: 1px;
			padding-left: 8px;
			width: 31%;

			@media screen and (max-width: $ports-max-mob ) {
				padding-left: 0;
				width: 25%;
			}
		}

		&.table--orders__date {
			width: 25%;

			@media screen and (max-width: $ports-max-mob ) {
				width: 35%;
			}
		}

		&.table--orders__status {
			width: 33%;

			@media screen and (max-width: $ports-max-mob ) {
				width: 29%;
			}
		}

		&.table--orders__total {
			width: 11%;
		}
	}

	tr td {position: relative;}

	tr td:after,
	tr:first-child th:after,
	tr:first-child td:after {
		content: "";
		display: block;
		position: absolute;
		bottom: 0;
		border-bottom: 1px solid rgba(151,151,151,.17);
		left: 0;
		right: 0;
	}
	tr:first-child th:first-child:after,
	tr:first-child td:first-child:after {
		left: 0;
		right: 0;
	}
}

.account-addresses {
	display: flex;
	justify-content: space-between;
	margin-bottom: 41px;

	@media screen and (max-width: $ports-min-desk ) {
		flex-wrap: wrap;
	}

	@media screen and (max-width: $ports-max-mob ) {
		display: block;
		margin-bottom: 0;

		&:last-of-type,
		&.last {
			margin-bottom: -22px;
		}
	}


	br {
		display: block;
	}
}

.account-addresses__item {
	width: 274px;

	@media screen and (max-width: $ports-max-mob ) {
		margin-bottom: 41px;
	}
}

.account-addresses__item-title {
	display: block;
	font-size: 12px;
	font-weight: 500;
	letter-spacing: 1px;
	line-height: 18px;
	text-transform: uppercase;
	margin-bottom: 16px;
}

.account-addresses__item-text {
	display: block;
	font-size: 14px;
	font-weight: 300;
	line-height: 24px;
	font-style: normal;
	margin-bottom: 14px;
}

.account-addresses__item-links {
	a {
		margin-right: 18px;
	}
}


/*========================================

  GENERAL PAGES

========================================*/
.general-page--wrapper {
	padding: 80px 0 59px;
	min-height: 705px;
	max-width: 800px;
	margin: 0 auto;

	&.contact-page--wrapper {
		max-width: none;
	}

	@media screen and (max-width: $ports-max-mob ) {
		min-height: 717px;
		padding: 48px 0 80px;

		&.contact-page--wrapper {
			padding: 71px 0 80px;
		}
	}

	&:not(.contact-page--wrapper) {
		.section-header {
			margin-bottom: 38px;

			@media screen and (max-width: $ports-max-mob ) {
				margin-bottom: 20px;
			}
		}
	}

	h1, .h1,
	h2, .h2,
	h3, .h3,
	h4, .h4,
	h6, .h6, {
		font-weight: 500;
		letter-spacing: 4px;
	}

	.rte {

		h1, .h1,
		h2, .h2,
		h3, .h3,
		h4, .h4,
		h6, .h6, {
			font-family: "stevie-sans",sans-serif;
			text-transform: none;
			font-weight: 300;
			letter-spacing: -0.07px;
			margin-bottom: .4em;

			@media screen and (max-width: $ports-max-mob ) {
				margin-bottom: .75em;
			}
		}

		strong {
			font-size: 12px;
			font-weight: 500;
			letter-spacing: 1px;
			line-height: 18px;
			text-transform: uppercase;
		}

		p, ul, ol, table {
			letter-spacing: -0.21px;
			margin-bottom: 24px;
			font-weight: 300;
		}

		br {
			display: block;

			& + br {
				display: none;
			}
		}

		@media screen and (max-width: $ports-max-mob ) {
			& > br + br {
				display: block;
			}
		}

	}
}


.contact-page__header {
	margin-bottom: 66px;

	@media screen and (max-width: $ports-max-mob ) {
		margin-bottom: 33px;
	}

	.section-header__title {
		margin-bottom: 9px;
	}
}
