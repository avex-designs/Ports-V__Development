/*========================================

  PHASE 3 GENERAL

========================================*/
.underlined-link {
	display: inline-block;
	border-bottom: 1px solid #000;
	line-height: 1.25rem;
	font-weight: 300;
	margin-bottom: 0.25rem;
}

.marked {
	text-transform: uppercase;
	font-size: 12px;
	font-weight: 500;
	letter-spacing: 1px;
	line-height: 18px;
}

.floated-label {
	text-transform: uppercase;
	position: absolute;
	top: 50%;
	left: 0;
	transform: translateY(-50%);
	pointer-events: none;
	font-style: normal;
	font-size: 12px;
	font-weight: 300;
	letter-spacing: 1px;
	line-height: 18px;
	@include transition(all .3s ease);

	.child-input-focused &,
	.child-input-focused &.label-for-textarea,
	textarea:focus + &
	input:focus + & {
		top: -2px;
		transform: translateY(-100%);
	}

	&.label-for-textarea {
		top: 0;
		transform: translateY(0);
	}

}

.icon-eye:before {
	content: "";
	display: inline-block;
	background: url({{ "eye.svg" | asset_url }});
	width: 20px;
	height: 12px;
}

.toggle-password {
	position: absolute;
	bottom: 16px;
	right: 9px;
	opacity: .5;

	& + input {
		padding-right: 25px;
	}

	.child-input-focused & {
		opacity: 1;
	}

	&.shown:after {
		content: "";
		display: block;
		position: absolute;
		background: #000;
		width: 22px;
		height: 2px;
		top: calc( 50% - 2px );
		transform: rotate(-35deg) translateY(-50%);
	}
}

.checkbox-group {
	font-style: normal;
	display: block;
	text-align: left;
	position: relative;
	padding-left: 26px;
	cursor: pointer;
	margin: 18px 0 -23px;

	@media screen and (max-width: $ports-max-mob ) {
		margin-bottom: 0;
	}

	&:before,
	.checkbox-label:before {
		content: "";
		display: inline-block;
		position: absolute;
		top: 50%;
		left: 0;
		transform: translateY(-50%);
	}

	&:before {
		width: 16px;
		height: 16px;
		border: 1px solid #000;
	}

	.checkbox-label:before {
		display: none;
		width: 8px;
		height: 8px;
		left: 4px;
		border-radius: 50%;
		background: #000;
	}

	input[type="checkbox"] {
		display: none;

		&:checked + .checkbox-label:before {
			display: inline-block;
		}
	}
}

.vertical-nav {
	margin-bottom: 20px;
}

.vertical-nav_item {
	margin-bottom: 0.75em;
}
.vertical-nav_link {
	font-size: 12px;
	font-weight: 300;
	letter-spacing: 1px;
	line-height: 18px;
	text-transform: uppercase;
	opacity: 0.5;

	&:hover,
	&.active {
		opacity: 1;
	}
}

