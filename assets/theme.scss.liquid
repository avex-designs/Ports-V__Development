#shopify-section-product-template {
  padding-top: $gutters;

  .product-single {
      display: flex;

      &>* {  width: 100%; }

      .product_images_container {
          display: inline-flex;

          .spin-figure img {  position: relative; }

          &>* {  flex: 1 1 auto; }

          .product-thumbnail__photos {  padding: 0 5rem 0 1.75rem; }

          .slick-vertical .slick-slide { border: 0; }

          .product-single__thumbnails {
              margin: 0;

              li {  margin-bottom: 1rem; }
          }

          #thumbnails_container {
              max-width: 120px;

              .product-single__thumbnail img {
                  border: 0;
              }

              .product-single__thumbnail img {  opacity: .7;  }
              .product-single__thumbnail.active-thumb img {  opacity: 1;  }

              .image_360:before {
                  content: ' ';
                  display: inline-block;
                  background-image: url( {{ '360-image-icon.svg' | file_url }} );
                  background-repeat: no-repeat;
                  position: absolute;
                  background-position: 50% 75%;
                  top: 0;
                  left: 0;
                  height: 100%;
                  width: 100%;
                  z-index: 1;
              }

              .arrows_append {
                  display: flex;
                  justify-content: center;

                  .slick-prev,
                  .slick-next {
                      font-size: 0;
                      margin: .5rem;

                      &:before {
                          content: ' ';
                          display: block;
                          width: 1.5625rem;
                          height: 0.8125rem;
                          background-repeat: no-repeat;
                          background-position: center;
                          background-size: contain;
                      }

                      &.slick-disabled {
                          /* display: none !important; */
                          opacity: .4;
                      }
                  }

                  .slick-next:before {  background-image: url( {{'thumbs-slider_down.svg' | file_url }} ) }
                  .slick-prev:before {  background-image: url( {{'thumbs-slider-up.svg' | file_url }} ); }
              } /* arrows_append */
          } /* #thumbnails_container  */

      } /* product_images_container */

      .product-single__meta--wrapper {
          max-width: 30rem;
          padding: 0;

          .product-single__meta {
              padding-top: 0;
              margin-top: 0px;
              -webkit-transition: margin .25s; /* For Safari 3.1 to 6.0 */
              transition: margin .25s;

              &.scroll-to-fixed-fixed {
                margin-top: 60px;
              }

              .price-container {
                  margin-top: 1.5rem;
                  margin-bottom: 1rem;

                  &>span {
                    font-size: 1.5rem;
                    line-height: 1.3;
                  }
              } /* price-container */

              .product-single__description {
                  margin-top: 1.5rem;
                  margin-bottom: 0;

                  ul {
                      list-style: none;

                      li {  position: relative; }

                      li:before {
                          content: ' ';
                          display: inline-block;
                          width: 0.6875rem;
                          height: 100%;
                          background-image: url( {{ 'Checkmark.svg' | file_url }} );
                          position: absolute;
                          right: calc(100% + 8px );
                          background-position: center;
                          background-size: contain;
                          background-repeat: no-repeat;
                      }
                  }
              } /* product-single__description */

              .size-guide {
                  margin-top: 1rem;
                  display: inline-block;
                  margin-bottom: .75rem;

                  &:after {
                      display: block;
                      content: '';
                      border-bottom: solid 1px #222323;
                      margin-bottom: -1px;
                  }
              } /* size-guide */

              .social-share-container {
                  display: flex;
                  align-items: center;

                  .social-share-trigger {
                      cursor: pointer;
                  }

                  .share-image {
                      margin-left: .75rem;
                  }

                  .share-title,
                  .share-image,
                  .social-sharing {
                      display: none;
                  }

                  .social-sharing a {
                     line-height: 1;
                  }

                  .share-image.active,
                  .social-sharing.active {
                      display: initial;
                  }

                  .icon-email-share:before,
                  .icon-facebook-share:before,
                  .icon-twitter-share:before,
                  .icon-pinterest-share:before {
                      content: ' ';
                      display: block;
                      width: 1rem;
                      height: 1rem;
                      background-position: center;
                      background-size: contain;
                      background-repeat: no-repeat;
                  }

