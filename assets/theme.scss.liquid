.burger-icon--top {
  top: 17px;
  top: 21px;

  .js-drawer-open-left & {
    top: 25px;
    @include transform(rotate(45deg));
  }
}

.burger-icon--mid {
  top: 24px;

  .js-drawer-open-left & {
    opacity: 0;
  }
}

.burger-icon--bottom {
  top: 31px;
  top: 28px;

  .js-drawer-open-left & {
    top: 25px;
    @include transform(rotate(-45deg));
  }
